#include "stdio.h
#include "fish_action.h"

void label (struct fish fish) {
	printf("Name: %s\nSpecies: %s\n%i age, %i teeth\n", fish.name, fish.species, fish.age, fish.teeth);
	printf("Give %2.2f kg %s and allow %s during %2.2f hours", fish.care.food.weight, fish.care.food.ingredients, fish.care.exersize.description, fish.care.exersize.duration);
}
